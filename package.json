{
  "name": "scaffold",
  "version": "1.0.0",
  "description": "Scaffold for NS-MS",
  "private": true,
  "main": "dist/app.js",
  "scripts": {
    "start": "NODE_ENV=production node ./dist/app.js",
    "tsc": "tsc",
    "pretest": "tsc",
    "test": "rm -rf dist/ && npm run build && NODE_ENV=test nodemon --exec 'mocha dist/tests'",
    "watch": "mocha-typescript-watch",
    "prepare": "tsc",
    "postinstall": "npm run build",
    "dev": "NODE_ENV=development nodemon src/app.ts",
    "build": "tsc -p .",
    "deploy": "rm -rf dist/ && npm run build && serverless deploy",
    "offline": "rm -rf dist/ && npm run build && sls offline"
  },
